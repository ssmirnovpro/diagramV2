{
  "name": "uml-api-service",
  "version": "1.0.0",
  "description": "API Service for UML diagram generation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "axios": "^1.5.0",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.1",
    "express-slow-down": "^2.0.1",
    "express-mongo-sanitize": "^2.2.0",
    "hpp": "^0.2.3",
    "winston": "^3.11.0",
    "prom-client": "^15.1.0",
    "redis": "^4.6.0",
    "compression": "^1.7.4",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "joi": "^17.11.0",
    "bull": "^4.12.0",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}